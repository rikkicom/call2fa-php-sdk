{
  "name": "rikkicom/call2fa",
  "description": "This is a client for the Rikkicom's Call2FA service",
  "keywords": [
    "calls",
    "call2factor",
    "verification"
  ],
  "license": "MIT",
  "type": "library",
  "require": {
    "php": ">=8.2",
    "guzzlehttp/guzzle": "^7.0",
    "ext-json": "*"
  },
  "autoload": {
    "psr-4": {
      "Rikkicom\\Call2FA\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Rikkicom\\Call2FA\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^12.4"
  }
}
